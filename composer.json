{
    "name": "typiqally/enum",
    "description": "PHP Enum Types",
    "keywords": [
        "typiqally",
        "enum",
        "enumerable"
    ],
    "homepage": "https://github.com/Typiqally/enum",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Jelle Maas",
            "email": "typiqally@gmail.com",
            "homepage": "https://typiqally.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Typiqally\\Enum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Typiqally\\Enum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "support": {
        "issues": "https://github.com/Typiqally/enum/issues",
        "source": "https://github.com/Typiqally/enum"
    }
}
